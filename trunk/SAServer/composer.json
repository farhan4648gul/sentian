{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thetiby/larry-four-generator"
        },
        {
            "type": "vcs",
            "url": "https://github.com/thetiby/php-nlp-tools"
        }
    ],
	"require": {
		"laravel/framework": "4.0.*",
		"mashape/unirest-php" : "dev-master",
		"phpunit/phpunit": "3.7.*",
		"raahul/larryfour": "dev-master",
    	"lisachenko/go-aop-php": "dev-master",
    	"nlp-tools/nlp-tools": "1.0.*@dev"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/externalLibs/phpInsight",
			"app/classes"
		],
        "psr-0": {
            "Aspect": "app/aspects"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
